{
	"name": "modelsmith-fe",
	"version": "1.0.0",
	"author": "Pop Ioan (John)",
	"config": {
		"docker_image_name": "ms-frontend",
		"docker_container_name": "ms-frontend-container",
		"docker_container_port": "4200"
	},
	"scripts": {
		"ng": "ng",
		"start": "ng serve --proxy-config proxy.conf.json",
		"build:prod": "ng build --configuration production",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"prettier:fix": "npx prettier --write . --config ./.prettierrc.js",
		"docker:deploy": "npm run docker:stop-container && npm run docker:remove-container && npm run docker:build-image && npm run docker:remove-dangling && npm run docker:run-container",
		"docker:build-image": "docker build -t ${npm_package_config_docker_image_name} .",
		"docker:stop-container": "docker stop ${npm_package_config_docker_container_name} || true",
		"docker:remove-container": "docker rm ${npm_package_config_docker_container_name} || true",
		"docker:run-container": "docker run -d --name ${npm_package_config_docker_container_name} -p ${npm_package_config_docker_container_port}:${npm_package_config_docker_container_port} ${npm_package_config_docker_image_name}",
		"docker:remove-dangling": "docker image prune -f",
		"docker:restart": "docker stop ${npm_package_config_docker_container_name} && docker start ${npm_package_config_docker_container_name}",
		"docker:copy-index": "docker cp server/index.js ${npm_package_config_docker_container_name}:/app/frontend/server/index.js",
		"check:circular-dependencies": "npx madge --circular --extensions ts ./"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^18.1.0",
		"@angular/cdk": "^18.0.6",
		"@angular/common": "^18.1.0",
		"@angular/compiler": "^18.1.0",
		"@angular/core": "^18.1.0",
		"@angular/forms": "^18.1.0",
		"@angular/material": "^18.0.6",
		"@angular/platform-browser": "^18.1.0",
		"@angular/platform-browser-dynamic": "^18.1.0",
		"@angular/router": "^18.1.0",
		"@lottiefiles/dotlottie-web": "^0.28.0",
		"@ngneat/until-destroy": "^10.0.0",
		"@ngrx/effects": "^18.0.1",
		"@ngrx/operators": "^18.0.0",
		"@ngrx/store": "^18.0.1",
		"@xterm/addon-fit": "^0.10.0",
		"@xterm/addon-search": "^0.15.0",
		"@xterm/xterm": "^5.5.0",
		"chart.js": "^4.4.1",
		"chartjs-plugin-zoom": "^2.0.1",
		"jwt-decode": "^3.1.2",
		"ng2-charts": "^5.0.4",
		"ngrx-store-localstorage": "^18.0.0",
		"ngx-colors": "^3.6.0",
		"ngx-mat-select-search": "^7.0.6",
		"npm": "^10.9.0",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.14.7"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^18.0.7",
		"@angular/cli": "~18.0.7",
		"@angular/compiler-cli": "^18.1.0",
		"@lottiefiles/lottie-types": "^1.4.1",
		"@ngrx/store-devtools": "^18.0.1",
		"@types/jasmine": "~4.3.0",
		"autoprefixer": "^10.4.15",
		"jasmine-core": "~4.6.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"postcss": "^8.4.32",
		"prettier": "^3.0.3",
		"tailwindcss": "^3.3.3",
		"typescript": "~5.4.5"
	}
}
